<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - devdap Responsive Admin Theme</title>
    <link rel="icon" href="~/images/favicons.ico" />

    <!--Render Main Stylesheets-->
    @Styles.Render("~/Content/css")

   
</head>
<body class="light-skin fixed-navbar sidebar-scroll">
    <!-- Render Navigation Layouts-->
    @Html.Partial("_Navigation")

    <!--Render Sidebar-->
    @Html.Partial("_Sidebar")

    <div id="wrapper">
        @RenderBody()

        <!--Render Footer -->
        <footer class="footer">
            <span class="pull-right">
                Exclusive Admin
            </span>
            Powered By devdap.com
        </footer>
    </div>

    @*Primary Javascript Files for Every Page*@
    @Scripts.Render("~/Scripts/vendor/jquery/dist/jquery.min.js")
    @Scripts.Render("~/Scripts/vendor/jquery-ui/jquery-ui.min.js")
    @Scripts.Render("~/Scripts/vendor/slimScroll/jquery.slimscroll.min.js")
    @Scripts.Render("~/Scripts/vendor/bootstrap/dist/js/bootstrap.min.js")
    @Scripts.Render("~/Scripts/vendor/metisMenu/dist/metisMenu.min.js")
    @Scripts.Render("~/Scripts/vendor/iCheck/icheck.min.js")
    @Scripts.Render("~/Scripts/vendor/sparkline/index.js")
    @Scripts.Render("~/Scripts/vendor/peity/jquery.peity.min.js")

    @*Main JS Files*@
    @Scripts.Render("~/Scripts/devdap.js")
    @Scripts.Render("~/Scripts/charts.js")

    <script>$(function () { $("#sparkline7").sparkline([34, 43, 43, 35, 44, 32, 44, 52, 25], { type: "line", lineColor: "#54ab2c", fillColor: "#C97CE5", }); $("#sparkline2").sparkline([5, 6, 7, 2, 0, -4, -2, 4], { type: "bar", barColor: "#C97CE5", negBarColor: "#c6c6c6" }); $("#sparkline3").sparkline([1, 1, 2], { type: "pie", sliceColors: ["#C97CE5", "#b3b3b3", "#e4f0fb"] }); $("#sparkline4").sparkline([34, 43, 43, 35, 44, 32, 15, 22, 46, 33, 86, 54, 73, 53, 12, 53, 23, 65, 23, 63, 53, 42, 34, 56, 76, 15, 54, 23, 44], { type: "line", lineColor: "#C97CE5", fillColor: "#ffffff", }); $("#sparkline5").sparkline([1, 1, 0, 1, -1, -1, 1, -1, 0, 0, 1, 1], { type: "tristate", posBarColor: "#C97CE5", negBarColor: "#bfbfbf" }); $("#sparkline6").sparkline([4, 6, 7, 7, 4, 3, 2, 1, 4, 4, 5, 6, 3, 4, 5, 8, 7, 6, 9, 3, 2, 4, 1, 5, 6, 4, 3, 7,], { type: "discrete", lineColor: "#C97CE5" }) });</script>
    @RenderSection("scripts", required: false)
</body>
                    </html>
