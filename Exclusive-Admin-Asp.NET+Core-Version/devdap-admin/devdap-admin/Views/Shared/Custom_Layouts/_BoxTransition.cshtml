<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - devdap Responsive Admin Theme</title>
    <link rel="icon" href="~/images/favicons.ico" />

    <!--Render Main Stylesheets-->
    @Styles.Render("~/Content/css")

</head>
<body class="light-skin fixed-navbar sidebar-scroll">
    <!-- Render Navigation Layouts-->
    @Html.Partial("_Navigation")

    <!--Render Sidebar-->
    @Html.Partial("_Sidebar")

    <div id="wrapper">
        @RenderBody()

        <!--Render Footer -->
        <footer class="footer">
            <span class="pull-right">
                Exclusive Admin
            </span>
            Powered By devdap.com
        </footer>
    </div>

    @*Primary Javascript Files for Every Page*@
    @Scripts.Render("~/Scripts/vendor/jquery/dist/jquery.min.js")
    @Scripts.Render("~/Scripts/vendor/jquery-ui/jquery-ui.min.js")
    @Scripts.Render("~/Scripts/vendor/slimScroll/jquery.slimscroll.min.js")
    @Scripts.Render("~/Scripts/vendor/bootstrap/dist/js/bootstrap.min.js")
    @Scripts.Render("~/Scripts/vendor/metisMenu/dist/metisMenu.min.js")
    @Scripts.Render("~/Scripts/vendor/iCheck/icheck.min.js")
   
    @Scripts.Render("~/Scripts/vendor/sparkline/index.js")

    @*Main JS Files*@
    @Scripts.Render("~/Scripts/devdap.js")
   

    <script>$(function () { $(".animate-panel-demo").demoStagger(); $(".runAnimation").click(function () { var c = $(".setEffect").attr("data-effect"); var d = $(".animation-select").val(); var b = $("#acontainer").html(); var a = b.replace(new RegExp(c, "g"), d); $(".runIt").remove(); setTimeout(function () { $("#acontainer").append(a) }) }) }); $.fn.demoStagger = function () { var e = $(this); var i = $(this).data("effect"); var f = $(this).data("delay"); var d = $(this).data("child"); if (!i) { i = "zoomIn" } if (!f) { f = 0.06 } else { f = f / 10 } if (!d) { d = ".row > div" } else { d = "." + d } var g = 0; var c = Math.abs(f) + g; var a = e.find(d); a.addClass("opacity-0"); a = e.find(d); a.addClass("animated-panel").addClass(i); var b = a.length + 10; var h = (b * f * 10000) / 10; a.each(function (k, l) { c += f; var j = Math.round(c * 10) / 10; $(l).css("animation-delay", j + "s"); $(l).removeClass("opacity-0") }) };</script>

    @RenderSection("scripts", required: false)
</body>
</html>
