<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - devdap Responsive Admin Theme</title>
    <link rel="icon" href="~/images/favicons.ico" />

    <!--Render Main Stylesheets-->
    @Styles.Render("~/Content/css")

    <link rel="stylesheet" href="~/Scripts/vendor/fooTable/css/footable.core.min.css" />
    <link rel="stylesheet" href="~/Scripts/vendor/datatables.net-bs/css/dataTables.bootstrap.min.css" />
</head>
<body class="light-skin fixed-navbar sidebar-scroll">
    <!-- Render Navigation Layouts-->
    @Html.Partial("_Navigation")

    <!--Render Sidebar-->
    @Html.Partial("_Sidebar")

    <div id="wrapper">
        @RenderBody()

        <!--Render Footer -->
        <footer class="footer">
            <span class="pull-right">
                Exclusive Admin
            </span>
            Powered By devdap.com
        </footer>
    </div>

    @*Primary Javascript Files for Every Page*@
    @Scripts.Render("~/Scripts/vendor/jquery/dist/jquery.min.js")
    @Scripts.Render("~/Scripts/vendor/jquery-ui/jquery-ui.min.js")
    @Scripts.Render("~/Scripts/vendor/slimScroll/jquery.slimscroll.min.js")
    @Scripts.Render("~/Scripts/vendor/bootstrap/dist/js/bootstrap.min.js")
    @Scripts.Render("~/Scripts/vendor/metisMenu/dist/metisMenu.min.js")
    @Scripts.Render("~/Scripts/vendor/iCheck/icheck.min.js")
    @Scripts.Render("~/Scripts/vendor/fooTable/dist/footable.all.min.js")
    @Scripts.Render("~/Scripts/vendor/sparkline/index.js")

    @*Main JS Files*@
    @Scripts.Render("~/Scripts/devdap.js")



    @Scripts.Render("~/Scripts/vendor/datatables/media/js/jquery.dataTables.min.js")
    @Scripts.Render("~/Scripts/vendor/datatables.net-bs/js/dataTables.bootstrap.min.js")
    @Scripts.Render("~/Scripts/vendor/pdfmake/build/pdfmake.min.js")
    @Scripts.Render("~/Scripts/vendor/pdfmake/build/vfs_fonts.js")
    @Scripts.Render("~/Scripts/vendor/datatables.net-buttons/js/buttons.html5.min.js")
    @Scripts.Render("~/Scripts/vendor/datatables.net-buttons/js/buttons.print.min.js")
    @Scripts.Render("~/Scripts/vendor/datatables.net-buttons/js/dataTables.buttons.min.js")
    @Scripts.Render("~/Scripts/vendor/datatables.net-buttons-bs/js/buttons.bootstrap.min.js")
    @Scripts.Render("~/Scripts/scripts/devdap.js")

    <script>$(function () { $("#example1").footable(); $("#example2").footable() });</script>
    <script>$(function () { $("#example1").dataTable({ ajax: "api/datatables.json", dom: "<'row'<'col-sm-4'l><'col-sm-4 text-center'B><'col-sm-4'f>>tp", lengthMenu: [[10, 25, 50, -1], [10, 25, 50, "All"]], buttons: [{ extend: "copy", className: "btn-sm" }, { extend: "csv", title: "ExampleFile", className: "btn-sm" }, { extend: "pdf", title: "ExampleFile", className: "btn-sm" }, { extend: "print", className: "btn-sm" }] }); $("#example2").dataTable() });</script>

    @RenderSection("scripts", required: false)
</body>
</html>
